  connect_bd_intf_net -intf_net mem_interconnect_M00_AXI [get_bd_intf_pins mem_interconnect/M00_AXI] [get_bd_intf_pins mig_7series_0/S_AXI]
  connect_bd_intf_net -intf_net pcie_mem_periph_split_M00_AXI [get_bd_intf_pins mem_interconnect/S00_AXI] [get_bd_intf_pins pcie_mem_periph_split/M00_AXI]
  connect_bd_intf_net -intf_net pcie_mem_periph_split_M01_AXI [get_bd_intf_pins pcie_mem_periph_split/M01_AXI] [get_bd_intf_pins periph_interconnect/S00_AXI]
  connect_bd_intf_net -intf_net periph_interconnect_M00_AXI [get_bd_intf_pins axi_bram_ctrl_0/S_AXI] [get_bd_intf_pins periph_interconnect/M00_AXI]
  connect_bd_intf_net -intf_net periph_interconnect_M01_AXI [get_bd_intf_pins axi_hwicap_0/S_AXI_LITE] [get_bd_intf_pins periph_interconnect/M01_AXI]
